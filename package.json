{
  "name": "portfolio_site",
  "version": "1.0.0",
  "description": "Personal Portfolio Website",
  "main": "index.html",
  "scripts": {
    "build:css": "npx tailwindcss -i ./css/tailwind.css -o ./css/main.css",
    "build:css:min": "npx tailwindcss -i ./css/tailwind.css -o ./css/main.min.css --minify",
    "watch:css": "npx tailwindcss -i ./css/tailwind.css -o ./css/main.css --watch",
    "dev": "npm run watch:css"
  },
  "devDependencies": {
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@tailwindcss/typography": "^0.5.16",
    "autoprefixer": "^10.4.19",
    "cssnano": "^7.1.0",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.4",
    "tailwindcss-animate": "^1.0.7",
    "tailwindcss-elevation": "^2.0.0",
    "tailwindcss-fluid-type": "^2.0.7"
  },
  "license": "MIT"
}
